@page
@model RazorPageVersion2022.Pages.Login.CreateLoginModel
@{
}


<form method="post">
    <div class="form-group">
        <label asp-for="@Model.User.UserName" class="control-lable"></label>
        <input asp-for="@Model.User.UserName" class="form-control" />
        <span asp-validation-for="@Model.User.UserName" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="@Model.User.Password" class="control-lable"></label>
        <input asp-for="@Model.User.Password" class="form-control" />
        <span asp-validation-for="@Model.User.Password" class="text-danger"></span>
    </div>

    <div class="form-group">
        <input type="submit" value="Create" class="btn btn-primary" />
    </div>
</form>